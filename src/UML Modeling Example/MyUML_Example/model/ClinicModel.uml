      <ownedBehavior xmi:type="uml:FunctionBehavior" xmi:id="_7AS3gN8pEe-AEN9jxhy8Pg" name="changeDoctorFacility" specification="_WWfpEN8pEe-AEN9jxhy8Pg">
        <language>INCLUDE</language>
        <language>CPP</language>
        <body>#include &quot;ClinicModel/SpecializedFacility.hpp&quot;&#xD;
#include &quot;ClinicModel/Staff.hpp&quot;</body>
        <body>//changes the workplace facility of a doctor&#xD;
std::shared_ptr&lt;Bag&lt;SpecializedFacility>> sfacility = this -> getSpecialfacilities();&#xD;
std::shared_ptr&lt;Bag&lt;Staff>> sTaff = this-> getStaffs();&#xD;
&#xD;
//find the correct facility for the given address&#xD;
//therefore, we iterate through all specialized facilities of the clinic&#xD;
&#xD;
for(Bag&lt;SpecializedFacility>::const_iterator it= sfacility->begin();it!= sfacility->end();it++)&#xD;
{&#xD;
	//removing a doctor from  from a specialized facilities (his previous workplace)&#xD;
	//check whether there is a doctor with the same name as the input&#xD;
	//(by iterating through all doctors and comparing their names)&#xD;
&#xD;
	std::shared_ptr&lt;Bag&lt;Staff>> doctors = (*it) -> getDoctors();&#xD;
&#xD;
	for(Bag&lt;Staff>::const_iterator staffs_it=doctors -> begin(); staffs_it!=doctors-> end(); staffs_it++)&#xD;
	{&#xD;
		//if there is a doctor with the same name, he is removed from the specialized facility&#xD;
		//(his previous workplace)&#xD;
&#xD;
		if ((*staffs_it)-> getName() == staff -> getName() &amp;&amp; (*staffs_it) -> getRole() == staff -> getRole()) &#xD;
		{&#xD;
			//removing the doctor from the doctors list from the facility&#xD;
			(*it) -> excludeDoctor(staff);&#xD;
			std::cout &lt;&lt; &quot;Removed employee &quot; &lt;&lt; staff -> getName() &lt;&lt; &quot; from &quot; &lt;&lt; (*it) -> getAddress() &lt;&lt; std::endl;&#xD;
			break;&#xD;
		}&#xD;
	}&#xD;
	//adding a doctor as new doctor to a facility (his new workplace)&#xD;
	//only if a specialized facility with the same address as the given address exists&#xD;
&#xD;
	if ((*it) -> getAddress() == specialFaciltylocation) &#xD;
	{&#xD;
		//adds the doctor to the list of doctors of the facility	&#xD;
		(*it) -> includeDoctor(staff);&#xD;
		 std::cout &lt;&lt; &quot;Added &quot; &lt;&lt;staff -> getRole()&lt;&lt;&quot; &quot;&lt;&lt; staff -> getName() &lt;&lt; &quot; to &quot; &lt;&lt; (*it) -> getAddress() &lt;&lt; std::endl;&#xD;
	}&#xD;
}</body>
      </ownedBehavior>
      <ownedBehavior xmi:type="uml:FunctionBehavior" xmi:id="_UfvtYN8qEe-AEN9jxhy8Pg" name="changeNurseFacility" specification="_dcFLsN8pEe-AEN9jxhy8Pg">
        <language>INCLUDE</language>
        <language>CPP</language>
        <body>#include &quot;ClinicModel/GeneralFacility.hpp&quot;&#xD;
#include &quot;ClinicModel/Staff.hpp&quot;</body>
        <body>std::shared_ptr&lt;Bag&lt;GeneralFacility>> gfacility = this -> getGeneralfacilities();&#xD;
std::shared_ptr&lt;Bag&lt;Staff>> sTaff = this-> getStaffs();&#xD;
&#xD;
for(Bag&lt;GeneralFacility>::const_iterator it=gfacility -> begin(); it!=gfacility -> end(); it++)&#xD;
{	&#xD;
	std::shared_ptr&lt;Bag&lt; Staff > > nurses = (*it) -> getNurses();&#xD;
	&#xD;
	for(Bag&lt;Staff>::const_iterator staffs_it=nurses -> begin(); staffs_it!=nurses -> end(); staffs_it++)&#xD;
	{&#xD;
		&#xD;
		if ((*staffs_it) -> getName() == staff -> getName() &amp;&amp; (*staffs_it) -> getRole() == staff -> getRole()) &#xD;
		{		&#xD;
			(*it) -> releaseNurse(staff);&#xD;
			std::cout &lt;&lt; &quot;Removed Nurse &quot; &lt;&lt; staff -> getName() &lt;&lt; &quot; from &quot; &lt;&lt; (*it) -> getAddress() &lt;&lt; std::endl;&#xD;
			break;&#xD;
		}&#xD;
	}&#xD;
&#xD;
	if ((*it) -> getAddress() == generalFaciltylocation) &#xD;
	{&#xD;
		//adds the person to the list of residents of the building&#xD;
		(*it) -> assignNurse(staff);&#xD;
		std::cout &lt;&lt; &quot;Added nurse &quot; &lt;&lt; staff -> getName() &lt;&lt; &quot; to &quot; &lt;&lt;(*it) -> getAddress() &lt;&lt; std::endl;&#xD;
	}&#xD;
}</body>
      </ownedBehavior>
      <ownedBehavior xmi:type="uml:FunctionBehavior" xmi:id="_nFnDAN8vEe-AEN9jxhy8Pg" name="printClinicDetails" specification="_hClJkN8vEe-AEN9jxhy8Pg">
        <language>INCLUDE</language>
        <language>CPP</language>
        <body>#include &quot;ClinicModel/Facility.hpp&quot;&#xD;
#include &quot;ClinicModel/Staff.hpp&quot;</body>
        <body>std::shared_ptr&lt; Bag&lt;SpecializedFacility> > specializedFacilities = this ->getSpecialfacilities();&#xD;
std::shared_ptr&lt; Bag&lt;GeneralFacility> > generalFacilities = this -> getGeneralfacilities();&#xD;
std::shared_ptr&lt; Bag&lt;AdministrationFacility> > adminFacilities = this -> getEmployAdmins();&#xD;
std::shared_ptr&lt; Bag&lt;Staff> > staffs = this -> getStaffs();&#xD;
&#xD;
std::cout &lt;&lt; &quot;========================================&quot; &lt;&lt; std::endl;&#xD;
std::cout &lt;&lt; &quot;Clinic&quot; &lt;&lt; this -> getClinicName() &lt;&lt; &quot; contains: &quot; &lt;&lt; std::endl;&#xD;
std::cout &lt;&lt; &quot;========================================&quot; &lt;&lt; std::endl;&#xD;
std::cout &lt;&lt; &quot;SpecializedFacilities:&quot; &lt;&lt; std::endl;&#xD;
std::cout &lt;&lt; &quot;----------------------------------------&quot; &lt;&lt; std::endl;&#xD;
&#xD;
for(Bag&lt;SpecializedFacility>::const_iterator it=specializedFacilities -> begin(); it!=specializedFacilities -> end(); it++)&#xD;
{&#xD;
	std::cout &lt;&lt; &quot;\tSpecial facilities with address \&quot;&quot; &lt;&lt; (*it) -> getAddress() &lt;&lt; &quot;\&quot; and specialization \&quot;&quot; &lt;&lt; (*it) -> getSpecialization() &lt;&lt; &quot;\&quot;&quot; &lt;&lt; std::endl;&#xD;
	std::cout &lt;&lt; &quot;\t\tDoctors:&quot; &lt;&lt; std::endl;&#xD;
	&#xD;
	//list of doctors in the current special facility&#xD;
	&#xD;
	std::shared_ptr&lt; Bag&lt;Staff> > staffs = (*it) -> getDoctors();&#xD;
	&#xD;
	&#xD;
	for(Bag&lt;Staff>::const_iterator itStaff = staffs -> begin(); itStaff!= staffs -> end(); itStaff++)&#xD;
	{&#xD;
		std::cout &lt;&lt; &quot;\t\t\tDoctor &quot; &lt;&lt; (*itStaff) -> getName() &lt;&lt; (*itStaff) -> getRole()&lt;&lt; std::endl;&#xD;
	}&#xD;
}&#xD;
&#xD;
std::cout &lt;&lt; &quot;General Facilities:&quot; &lt;&lt; std::endl;&#xD;
std::cout &lt;&lt; &quot;----------------------------------------&quot; &lt;&lt; std::endl;&#xD;
&#xD;
for(Bag&lt;GeneralFacility>::const_iterator it=generalFacilities -> begin(); it!=generalFacilities -> end(); it++)&#xD;
{&#xD;
	std::cout &lt;&lt; &quot;\tGeneral Facilities with address \&quot;&quot; &lt;&lt; (*it) -> getAddress() &lt;&lt; &quot;\&quot;&quot; &lt;&lt; std::endl;&#xD;
	std::cout &lt;&lt; &quot;\t\tNurses:&quot; &lt;&lt; std::endl;&#xD;
	&#xD;
	std::shared_ptr&lt; Bag&lt;Staff> > staffs = (*it) -> getNurses();&#xD;
	&#xD;
	&#xD;
	for(Bag&lt;Staff>::const_iterator itStaff=staffs -> begin(); itStaff!=staffs -> end(); itStaff++)&#xD;
	{&#xD;
		std::cout &lt;&lt; &quot;\t\t\tNurse &quot; &lt;&lt; (*itStaff) -> getName() &lt;&lt; (*itStaff) -> getRole ()&lt;&lt;std::endl;&#xD;
	}&#xD;
}&#xD;
&#xD;
std::cout &lt;&lt; &quot;Administration Facilities:&quot; &lt;&lt; std::endl;&#xD;
std::cout &lt;&lt; &quot;----------------------------------------&quot; &lt;&lt; std::endl;&#xD;
for(Bag&lt;AdministrationFacility>::const_iterator it=adminFacilities -> begin(); it!=adminFacilities -> end(); it++)&#xD;
{&#xD;
	std::cout &lt;&lt; &quot;\tAdministration Facilities with address \&quot;&quot; &lt;&lt; (*it) -> getAddress() &lt;&lt; &quot;\&quot;&quot; &lt;&lt; std::endl;&#xD;
	std::cout &lt;&lt; &quot;\t\tAdmins:&quot; &lt;&lt; std::endl;&#xD;
	&#xD;
	std::shared_ptr&lt; Bag&lt;Staff> > staffs = (*it) -> getAdmins();&#xD;
	&#xD;
	&#xD;
	for(Bag&lt;Staff>::const_iterator itStaff=staffs -> begin(); itStaff!=staffs -> end(); itStaff++)&#xD;
	{&#xD;
		std::cout &lt;&lt; &quot;\t\t\tAdmin &quot; &lt;&lt; (*itStaff) -> getName() &lt;&lt; (*itStaff) -> getRole ()&lt;&lt;std::endl;&#xD;
	}&#xD;
}&#xD;
&#xD;
&#xD;
std::cout &lt;&lt; &quot;Staffs in Clinic:&quot; &lt;&lt; std::endl;&#xD;
std::cout &lt;&lt; &quot;----------------------------------------&quot; &lt;&lt; std::endl;&#xD;
&#xD;
&#xD;
for(Bag&lt;Staff>::const_iterator it=staffs -> begin(); it!=staffs -> end(); it++)&#xD;
{	&#xD;
	std::cout &lt;&lt; &quot;\tStaff with name and Role: &quot; &lt;&lt; (*it) -> getName() &lt;&lt;&quot; &quot;&lt;&lt;(*it) -> getRole()&lt;&lt; std::endl;&#xD;
}&#xD;
std::cout &lt;&lt; &quot;========================================&quot;&lt;&lt; std::endl;&#xD;
std::cout &lt;&lt; &quot;Printed everything from Clinic&quot; &lt;&lt; this -> getClinicName() &lt;&lt; std::endl;&#xD;
std::cout &lt;&lt; &quot;========================================&quot;&lt;&lt; std::endl;</body>
